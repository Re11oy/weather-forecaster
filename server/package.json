{
  "name": "weather-forecast-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p src",
    "start": "ts-node src/server.ts",
    "watch": "nodemon src/server.ts",
    "clean": "rm build/*.*",
    "lint": "eslint 'src/*.ts'",
    "lint-fix": "yarn lint --fix",
    "run-prettier": "prettier 'src/*.ts'",
    "validate-prettiness": "yarn run-prettier -c",
    "make-prettier": "yarn run-prettier --write",
    "test": "yarn jest"
  },
  "dependencies": {
    "koa": "2.13.0",
    "koa-router": "9.4.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/koa": "2.11.4",
    "@types/koa-router": "7.4.1",
    "@types/node": "14.11.2",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "jest": "26.4.2",
    "nodemon": "2.0.4",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  }
}
