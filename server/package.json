{
  "name": "weather-forecast-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "start-worker": "ts-node src/forecast-update-worker.ts",
    "watch": "nodemon src/server.ts",
    "watch-worker": "nodemon src/forecast-update-worker.ts",
    "clean": "rm build/*.*",
    "lint": "eslint 'src/*.ts'",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier 'src/*.ts' -c",
    "prettier-fix": "prettier 'src/*.ts' --write",
    "test": "yarn jest --testPathIgnorePatterns integration-tests",
    "test-integration": "yarn jest src/integration-tests",
    "validate": "yarn lint && yarn prettier && yarn test"
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "fs-extra": "9.0.1",
    "koa": "2.13.0",
    "koa-router": "9.4.0",
    "mongoose": "5.10.9",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.2",
    "@types/jest": "26.0.14",
    "@types/koa": "2.11.5",
    "@types/koa-router": "7.4.1",
    "@types/mongoose": "5.7.36",
    "@types/node": "14.11.8",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "jest": "26.6.0",
    "jest-fetch-mock": "3.0.3",
    "nodemon": "2.0.5",
    "prettier": "2.1.2",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  }
}
